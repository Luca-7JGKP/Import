<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/6.0/cronjob.xsd">
    <import>
        <cronjob name="com.lucaberwind.wcf.calendar.import.icalImport">
            <classname>wcf\system\cronjob\ICalImportCronjob</classname>
            <description>ICS Import (erweitert) - Mit UID-Mapping und Event-Threads</description>
            <description language="en">ICS Import (extended) - With UID mapping and event threads</description>
            <startminute>0,30</startminute>
            <starthour>*</starthour>
            <startdom>*</startdom>
            <startmonth>*</startmonth>
            <startdow>*</startdow>
            <canbeedited>1</canbeedited>
            <canbedisabled>1</canbedisabled>
        </cronjob>
        
        <cronjob name="com.lucaberwind.wcf.calendar.import.markPastRead">
            <classname>wcf\system\cronjob\MarkPastEventsReadCronjob</classname>
            <description>Markiert abgelaufene Events als gelesen</description>
            <description language="en">Marks past events as read</description>
            <startminute>10,40</startminute>
            <starthour>*</starthour>
            <startdom>*</startdom>
            <startmonth>*</startmonth>
            <startdow>*</startdow>
            <canbeedited>1</canbeedited>
            <canbedisabled>1</canbedisabled>
        </cronjob>
    </import>
</data>
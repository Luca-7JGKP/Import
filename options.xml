<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/6.0/option.xsd">
    <import>
        <categories>
            <category name="calendar.import">
                <parent>general</parent>
            </category>
        </categories>
        
        <options>
            <option name="calendar_import_ics_url">
                <categoryname>calendar.import</categoryname>
                <optiontype>text</optiontype>
                <defaultvalue></defaultvalue>
            </option>
            
            <option name="calendar_import_calendar_id">
                <categoryname>calendar.import</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>0</defaultvalue>
            </option>
            
            <option name="calendar_import_target_import_id">
                <categoryname>calendar.import</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>0</defaultvalue>
            </option>
            
            <option name="calendar_import_default_board_id">
                <categoryname>calendar.import</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>0</defaultvalue>
            </option>
            
            <option name="calendar_import_create_threads">
                <categoryname>calendar.import</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            
            <option name="calendar_import_convert_timezone">
                <categoryname>calendar.import</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            
            <option name="calendar_import_auto_mark_past_read">
                <categoryname>calendar.import</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            
            <option name="calendar_import_mark_updated_unread">
                <categoryname>calendar.import</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            
            <option name="calendar_import_max_events">
                <categoryname>calendar.import</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>100</defaultvalue>
                <minvalue>1</minvalue>
                <maxvalue>10000</maxvalue>
            </option>
            
            <option name="calendar_import_log_level">
                <categoryname>calendar.import</categoryname>
                <optiontype>select</optiontype>
                <defaultvalue>info</defaultvalue>
                <selectoptions>error:Nur Fehler
warning:Warnungen
info:Info
debug:Debug</selectoptions>
            </option>
        </options>
    </import>
</data>